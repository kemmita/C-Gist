#include <stdio.h>
#include <stdlib.h>
int main() {
    int x[4][3];
    for (int i = 0; i < 4; ++i) {
        for (int j = 0; j < 3; ++j) {
            x[i][j] = i;
        }
    }

    printf("Elements of the 2d array \n");
    for (int k = 0; k < 4; ++k) {
        for (int m = 0; m < 3; ++m) {
            printf("%4d", x[k][m]);
        }
        printf("\n");
    }

    return 0;
}
